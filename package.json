{
	"name": "@neovici/cosmoz-utils",
	"version": "3.0.3",
	"description": "[![Build Status](https://travis-ci.org/Neovici/cosmoz-utils.svg?branch=master)](https://travis-ci.org/Neovici/cosmoz-utils)",
	"keywords": [
		"polymer",
		"web-components"
	],
	"homepage": "https://github.com/neovici/cosmoz-utils#readme",
	"bugs": {
		"url": "https://github.com/neovici/cosmoz-utils/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/neovici/cosmoz-utils.git"
	},
	"license": "Apache-2.0",
	"author": "Neovici Development",
	"main": "index.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"analyze": "polymer analyze --input index.js > analysis.json",
		"lint": "eslint --cache --ext .js,.html . && polymer lint index.js lib/*.js",
		"pristine": "rm -rf node_modules/ && yarn install",
		"preversion": "npm run lint && npm run test -- --skip-plugin sauce",
		"postversion": "git push && git push --tags",
		"start": "polymer serve -o",
		"test": "LANGUAGE=sv polymer test"
	},
	"dependencies": {
		"@polymer/polymer": "^3.0.0",
		"moment": "^2.24.0"
	},
	"devDependencies": {
		"@neovici/eslint-config": "github:neovici/eslint-config#semver:^1.0.0",
		"@polymer/iron-component-page": "^4.0.0",
		"@polymer/iron-demo-helpers": "^3.0.0",
		"@polymer/iron-test-helpers": "^3.0.0",
		"@polymer/test-fixture": "^4.0.2",
		"@webcomponents/webcomponentsjs": "^2.0.0",
		"babel-eslint": "^10.0.2",
		"chai": "^4.2.0",
		"eslint": "^5.0.0",
		"eslint-plugin-html": "^5.0.0",
		"eslint-plugin-import": "^2.17.3",
		"eslint-plugin-mocha": "^5.0.0",
		"mocha": "^5.0.0",
		"polymer-cli": "^1.8.0",
		"wct-istanbul": "^0.14.3",
		"wct-mocha": "^1.0.0"
	}
}
